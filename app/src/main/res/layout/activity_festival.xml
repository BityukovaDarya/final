<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Festival"
    android:background="@color/fon">

    <ImageButton
        android:id="@+id/program"
        android:layout_width="158dp"
        android:layout_height="118dp"
        android:background="@color/fon"
        android:layout_marginLeft="200dp"
        android:layout_marginTop="50dp"
        app:srcCompat="@drawable/program1" />

    <TextView
        android:id="@+id/pr"
        android:layout_width="117dp"
        android:layout_height="40dp"
        android:layout_marginLeft="230dp"
        android:layout_marginTop="160dp"
        android:fontFamily="@font/lora"
        android:text="Программа"
        android:textColor="#000000"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/meik"
        android:layout_width="117dp"
        android:layout_height="40dp"
        android:layout_marginLeft="230dp"
        android:layout_marginTop="310dp"
        android:fontFamily="@font/lora"
        android:text="Мейкеры"
        android:textColor="#000000"
        android:textSize="20dp" />

    <ImageButton
        android:id="@+id/btn_fest"
        android:layout_marginTop="60dp"
        android:layout_marginLeft="50dp"
        android:layout_width="140dp"
        android:layout_height="118dp"
        android:background="@color/fon"
        app:srcCompat="@drawable/head2" />


    <TextView
        android:id="@+id/f"
        android:layout_width="158dp"
        android:layout_height="40dp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="160dp"
        android:fontFamily="@font/lora"
        android:text="О фестивале"
        android:textColor="#000000"
        android:textSize="20dp" />

    <ImageButton
        android:id="@+id/zone"
        android:layout_marginTop="200dp"
        android:layout_marginLeft="50dp"
        android:layout_width="158dp"
        android:layout_height="118dp"
        android:background="@color/fon"
        app:srcCompat="@drawable/zone2" />

    <TextView
        android:id="@+id/z"
        android:layout_width="158dp"
        android:layout_height="40dp"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="310dp"
        android:fontFamily="@font/lora"
        android:text="Зоны"
        android:textColor="#000000"
        android:textSize="20dp" />

    <ImageButton
        android:id="@+id/meiker"
        android:layout_width="158dp"
        android:layout_height="118dp"
        android:layout_marginTop="200dp"
        android:layout_marginLeft="200dp"
        android:background="@color/fon"
        app:srcCompat="@drawable/meikermenu" />


    <ImageButton
        android:id="@+id/partner"
        android:layout_marginTop="350dp"
        android:layout_marginLeft="50dp"
        android:layout_width="158dp"
        android:layout_height="118dp"
        android:background="@color/fon"
        app:srcCompat="@drawable/partnermenu" />

    <TextView
        android:id="@+id/p"
        android:layout_width="158dp"
        android:layout_height="40dp"
        android:layout_marginLeft="70dp"
        android:layout_marginTop="450dp"
        android:fontFamily="@font/lora"
        android:text="Партнеры"
        android:textColor="#000000"
        android:textSize="20dp" />

    <ImageButton
        android:id="@+id/meropr"
        android:layout_width="158dp"
        android:layout_height="118dp"
        android:layout_marginTop="350dp"
        android:layout_marginLeft="200dp"
        android:background="@color/fon"
        app:srcCompat="@drawable/meropr" />

    <TextView
        android:id="@+id/mer"
        android:layout_width="158dp"
        android:layout_height="40dp"
        android:layout_marginLeft="200dp"
        android:layout_marginTop="450dp"
        android:fontFamily="@font/lora"
        android:text="Мероприятия"
        android:textColor="#000000"
        android:textSize="20dp" />


    <ImageButton
        android:id="@+id/kontakt"
        android:layout_width="158dp"
        android:layout_height="118dp"
        android:layout_marginTop="500dp"
        android:layout_marginLeft="50dp"
        android:background="@color/fon"
        app:srcCompat="@drawable/kontakt" />

    <TextView
        android:id="@+id/k"
        android:layout_width="158dp"
        android:layout_height="40dp"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="600dp"
        android:fontFamily="@font/lora"
        android:text="Контакты"
        android:textColor="#000000"
        android:textSize="20dp" />

    <ImageButton
        android:id="@+id/chat"
        android:layout_width="158dp"
        android:layout_height="118dp"
        android:layout_marginTop="500dp"
        android:layout_marginLeft="200dp"
        android:background="@color/fon"
        app:srcCompat="@drawable/spatialchat" />


    <TextView
        android:id="@+id/textView6"
        android:layout_width="158dp"
        android:layout_height="40dp"
        android:layout_marginLeft="260dp"
        android:layout_marginTop="600dp"
        android:fontFamily="@font/lora"
        android:text="Чат"
        android:textColor="#000000"
        android:textSize="20dp" />


    <Button
        android:id="@+id/konkurs"
        android:layout_width="212dp"
        android:layout_height="200dp"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="108dp"
        android:layout_marginTop="680dp"
        android:layout_marginBottom="22dp"
        android:background="@drawable/btnreg"
        android:drawableLeft="@drawable/ic_baseline_laptop_mac_24"
        android:fontFamily="@font/fjalla_one"
        android:text="Конкурс 'Создатели' " />

    <ImageButton
        android:id="@+id/backfest2"
        android:layout_width="59dp"
        android:layout_height="48dp"
        android:background="@color/white"
        android:backgroundTint="@color/fon"
        app:srcCompat="@drawable/ic_baseline_arrow_back_24" />



</RelativeLayout>


